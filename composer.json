{
    "name": "vkm-apps/x-form",
    "description": "Reusable Laravel Blade Form Components using Livewire & AlpineJS.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasilis Milopoulos",
            "email": "vasilis.milopoulos@gmail.com"
        }
    ],
    "keywords": ["Laravel", "livewire", "html forms", "form layouts", "blade", "components", "alpinejs"],
    "autoload": {
        "psr-4": {
            "VkmApps\\XForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VkmApps\\XForm\\XFormServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
